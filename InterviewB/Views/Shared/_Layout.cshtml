<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>مجلس الهيئة للكليات العسكرية والمعهد الفني للقوات المسلحة</title>

    <!-- Boostrap CSS Style -->
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <!-- Custom Main Style -->
    <link rel="stylesheet" href="~/Content/main.css" />

    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
    <!-- Begin Of WebSite -->
    <div class="wrapper">
        @RenderBody()
    </div>


    <!-- jQuery & jQuery-UI -->
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/jquery-ui.min.js"></script>

    <!-- Touch-Punch JS To support Touchable Divices -->
    <script src="~/Scripts/jquery.ui.touch-punch.min.js"></script>
    <!-- Pooper JS -->
    <script src="~/Scripts/popper.min.js"></script>
    <!-- Bootstrap JS -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!--SignalR-js-->
    <script src="~/Scripts/jquery.signalR-2.4.1.min.js"></script>
    <!--AutoGenerated Script For SignalR-->
    <script src="~/signalr/js"></script>
    <!-- Sortable JS -->
    <script src="~/Scripts/jquery-sortable.js"></script>
    <!-- Fontawesome JS -->
    <script src="~/Scripts/all.js"></script>
    <!-- EnglishToArabicNumbers -->
    <script src="~/Scripts/persianNum.jquery-2.min.js"></script>
    <!-- Custom Main JS -->
    <script src="~/Scripts/main.js"></script>


    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
