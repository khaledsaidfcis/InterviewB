@model InterviewB.Models.PARENTS_INFO
<li class="card my-2 order-1" id="parents-info" data-order="1">

    <h5 class="card-header text-white bg-success text-right d-block" data-toggle="collapse"
        data-target="#parents-info-body" aria-expanded="true" aria-controls="parents-info-body" id="parents-info-header">
        بيانات الوالدين
        <i class="fa fa-chevron-down"></i>
    </h5>

    <div id="parents-info-body" class="collapse show" aria-labelledby="parents-info-header">
        <div class="card-body">
            <div class="medical-table">
                <table class="table  table-bordered table-sm">
                    <tbody>
                        <tr>
                            <td rowspan="2" colspan="2">بيانات الأب</td>
                            <td>وقت الميلاد</td>
                            <td>@Model.FATHERGRADB</td>
                            <td>@Model.FAHERJOBB</td>
                        </tr>
                        <tr>
                            <td rowspan="2">حاليا</td>
                            <td>@Model.FATHERGRADN</td>
                            <td>@Model.FATHERJOBN</td>
                        </tr>
                        <tr>
                            <td>بيانات الأم</td>
                            <td>@Model.MOTHERNAME</td>
                            <td>@Model.MOTHERGRADN</td>
                            <td>@Model.MOTHERJOBN</td>
                        </tr>
                    </tbody>
                </table>
            </div>




        </div>
    </div>
</li>
